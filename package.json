{
  "name": "awesome-factory-associator",
  "version": "0.0.1",
  "description": "Sails hook aimed to aid unit testing by creating model instances and associations using Sequelize",
  "main": "index.js",
  "sails": {
    "isHook": true
  },
  "scripts": {
    "test": "node ./node_modules/mocha/bin/mocha test/bootstrap.test.js test/$MODULE.test.js --timeout $npm_package_config_timeout",
    "test:all": "node ./node_modules/mocha/bin/mocha test/bootstrap.test.js test --recursive --timeout $npm_package_config_timeout",
    "test:only": "node ./node_modules/mocha/bin/mocha test/bootstrap.test.js test --recursive --grep $MODULE --timeout $npm_package_config_timeout",
    "test:except": "node ./node_modules/mocha/bin/mocha test/bootstrap.test.js test --recursive --grep $MODULE -i --timeout $npm_package_config_timeout"
  },
  "config": {
    "timeout": 9000
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malpercio/awesome-factory-associator.git"
  },
  "keywords": [
    "sequelize",
    "fireline",
    "sails",
    "sailshook",
    "sails-hook",
    "testing",
    "model",
    "orm",
    "associations",
    "factory",
    "unittesting"
  ],
  "author": {
